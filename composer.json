{
    "name": "curly-deni/laravel-scopes",
    "description": "This is my package laravel-scopes",
    "keywords": [
        "laravel",
        "laravel-scopes"
    ],
    "homepage": "https://github.com/curly-deni/laravel-scopes",
    "license": "MIT",
    "authors": [
        {
            "name": "Danila Mikhalev",
            "email": "danila@dan-mi.ru",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "illuminate/container": "^8.12|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Aesis\\Scopes\\LaravelScopes\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aesis\\Scopes\\LaravelScopes\\LaravelScopesServiceProvider"
            ],
            "aliases": {
                "LaravelScopes": "Aesis\\Scopes\\LaravelScopes\\Facades\\LaravelScopes"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
